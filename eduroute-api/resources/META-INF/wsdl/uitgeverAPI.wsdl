<?xml version="1.0"?>
<definitions name="LicenseELOMatch" targetNamespace="urn:edurouteNamespace" xmlns:tns="urn:edurouteNamespace" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns="http://schemas.xmlsoap.org/wsdl/">
	<message name="maakBatchRequest">
		<part name="LeverancierCode" type="xsd:string"/>
		<part name="ControleCode" type="xsd:string"/>
		<part name="Omschrijving" type="xsd:string"/>
		<part name="DistributeurBatchnummer" type="xsd:string"/>
	</message>
	<message name="maakContentRequest">
		<part name="SessieID" type="xsd:string"/>
		<part name="EAN" type="xsd:string"/>
		<part name="Titel" type="xsd:string"/>
		<part name="Soort" type="xsd:string"/>
		<part name="URL" type="xsd:string"/>
		<part name="User" type="xsd:string"/>
		<part name="Password" type="xsd:string"/>
		<part name="BeschikbaarVanaf" type="xsd:string"/>
		<part name="DownloadPath" type="xsd:string"/>
		<part name="SCORMURL" type="xsd:string"/>
		<part name="SCORMLocatie" type="xsd:string"/>
		<part name="ActivatiecodeNodig" type="xsd:string"/>
	</message>
	<message name="verwijderContentRequest">
		<part name="SessieID" type="xsd:string"/>
