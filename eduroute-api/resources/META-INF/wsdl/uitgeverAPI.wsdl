			</output>
		</operation>
		<operation name="licenties">
			<soap:operation soapAction="urn:Magic-KeyPortType#licenties" style="rpc"/>
			<input>
				<soap:body use="literal" namespace="urn:myInputNamespace" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
			</input>
			<output>
				<soap:body use="literal" namespace="urn:myOutputNamespace" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
			</output>
		</operation>
		<operation name="getSchoolInfo">
			<soap:operation soapAction="urn:Magic-KeyPortType#getSchoolInfo" style="rpc"/>
			<input>
				<soap:body use="literal" namespace="urn:myInputNamespace" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
			</input>
			<output>
				<soap:body use="literal" namespace="urn:myOutputNamespace" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
			</output>
		</operation>
		<operation name="leverActivatiecodes">
			<soap:operation soapAction="urn:Magic-KeyPortType#leverActivatiecodes" style="rpc"/>
			<input>
				<soap:body use="literal" namespace="urn:myInputNamespace" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
			</input>
			<output>
				<soap:body use="literal" namespace="urn:myOutputNamespace" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
			</output>
		</operation>
	</binding>
	<service name="EdurouteDistributeurService">
		<documentation>
			Eduroute SOAP interface.
		</documentation>
		<port name="MyPort" binding="tns:KeyBinding">
			<soap:address location="http://www.eduroute.nl/soap/uitgever/uitgeverAPI.php"/>
		</port>
	</service>
</definitions>
